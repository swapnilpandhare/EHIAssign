@model IEnumerable<ContactInfo>
@{
    ViewData["Title"] = "Home Page";
}
<div class="text-center">
    <h4>Contact list</h4>
    <hr />
    <table cellpadding="0" cellspacing="0" border="1">
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email address</th>
            <th>Phone no.</th>
            <th>Gender</th>
        </tr>
        @foreach (ContactInfo contactInfo in Model)
        {
    <tr>
        <td>@contactInfo.FirstName</td>
        <td>@contactInfo.LastName</td>
        <td>@contactInfo.Email</td>
        <td>@contactInfo.PhoneNo</td>
        <td>@contactInfo.Gender</td>
        <td><a href="@Url.Action("CreateContact","Home", contactInfo)">Update</a></td>
    </tr>
        }
    </table>

</div>
